@model AcunMedyaAkademiPortfolio.Models.TblProfile

@{
    ViewBag.Title = "UpdateProfile";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Profil Güncelleme Sayfası</h2>
<form method="post">
    <label>Yeni Profil Id</label>
    <input type="text" class="form-control" name="Profileld" value="@Model.Profileld" />
    <br />
    <label>Yeni Profil Adı </label>
    <input type="text" class="form-control" name="Name" value="@Model.Name" />
    <br />
    <label>Yeni Profil Doğum Günü</label>
    <input type="text" class="form-control" name="Birtday" value="@Model.Birtday" />
    <br />
    <label>Yeni Profil Adres</label>
    <input type="text" class="form-control" name="Adrees" value="@Model.Adrees" />
    <label>Yeni Profil Mail</label>
    <input type="text" class="form-control" name="Email" value="@Model.Email" />
    <label>Yeni Profil Teelfon Numarası</label>
    <input type="text" class="form-control" name="Telefone" value="@Model.Telefone" />
    <button class="btn btn-info">Profil Güncelle</button>


</form>
