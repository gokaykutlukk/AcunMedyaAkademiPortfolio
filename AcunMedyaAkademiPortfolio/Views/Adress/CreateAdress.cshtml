@model AcunMedyaAkademiPortfolio.Models.TblAdress
@{
    ViewBag.Title = "CreateAdress";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni İletişim ekleme</h2>
<form method="post">
    <label>Id</label>
    @Html.TextBoxFor(x => x.AdressId, new { @class = "form-control" })
    <label>Adres</label>
    @Html.TextBoxFor(x => x.Adress, new { @class = "form-control" })
    <label>Mail</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <label>Telefon Numarası</label>
    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
    <label>Web Site</label>
    @Html.TextBoxFor(x => x.Website, new { @class = "form-control" })
    <button class="btn btn-info">İletişim Kaydet</button>

</form>

