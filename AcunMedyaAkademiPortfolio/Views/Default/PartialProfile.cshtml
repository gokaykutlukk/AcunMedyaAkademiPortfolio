@model List<AcunMedyaAkademiPortfolio.Models.TblProfile>
@foreach (var item in Model)
{
<ul class="about-info mt-4 px-md-0 px-2">
    <li class="d-flex"><span>İsim:</span> <span>@item.Name</span></li>
    <li class="d-flex"><span>Doğum Günü:</span> <span>@item.Birtday</span></li>
    <li class="d-flex"><span>Adres:</span> <span>@item.Adrees</span></li>
    <li class="d-flex"><span>Zip code:</span> <span>1000</span></li>
    <li class="d-flex"><span>Email:</span> <span>@item.Email</span></li>
    <li class="d-flex"><span>Telefon: </span> <span>@item.Telefone</span></li>
</ul>
}